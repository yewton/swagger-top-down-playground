  "schema" : {
    "$ref" : "#/definitions/Order"
  }
}
        
      ],
      "responses": {
        "200": {
  "description" : "発注成功",
  "schema" : {
    "$ref" : "#/definitions/Order"
  }
}
        ,
        "400": {
  "description" : "無効な注文"
}
        
        
      }
    } 
    
  } ,
  
  "/store/inventry": {
    
    "get": {
      "summary": "状況毎の在庫の確認",
