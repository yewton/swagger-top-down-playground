{
  "swagger": "2.0",
  "info": {
    "title": "Swagger ペットショップ (本番)",
    "description": "これはペットショップサーバの例です。\n\n[Swaggerについてもっと知る](http://swagger.io)、あるいは irc.freenode.net の IRC チャンネル `#swagger` に参加しましょう。\n\nこの例では、認可フィルタの確認用に `special-key` という API キーを利用できます。\n\n本番環境用の追加の説明文です。",
    "version": "1.0.0"
  },
  "produces": ["application/json"],
  "host": "yewton-swagger-playground.herokuapp.com",
  "basePath": "/api",
  "paths": {
  
  "/users": {
    
    "post": {
      "summary": "ユーザの作成",
      "description":"新規ユーザを作成します。",
      "x-swagger-router-controller": "User",
      "tags": ["User"],
      "operationId": "createUser",
      "parameters": [
        {
  "in" : "body",
  "name" : "body",
  "description" : "作成するユーザ",
  "required" : true,
  "schema" : {
    "$ref" : "#/definitions/User"
  }
}
        
      ],
      "responses": {
        "default": {
  "description" : "成功"
}
        
        
      }
    } 
    
  } ,
  
  "/user/login": {
    
    "get": {
      "summary": "システムへのログイン",
      "description":"ユーザをシステムにログインさせます。",
      "x-swagger-router-controller": "User",
      "tags": ["User"],
      "operationId": "loginUser",
      "parameters": [
        {
  "name" : "username",
  "in" : "query",
  "description" : "ログインユーザ名",
  "required" : true,
  "type" : "string"
},
        {
  "name" : "password",
  "in" : "query",
  "description" : "ログインパスワード",
  "required" : true,
  "type" : "string"
}
        
      ],
      "responses": {
        "200": {
  "description" : "成功",
  "schema" : {
    "type" : "string"
  },
  "headers" : {
    "X-Rate-Limit" : {
      "type" : "integer",
      "format" : "int32",
      "description" : "ユーザ毎の最大呼び出し数制限(毎時)"
    },
    "X-Expires-After" : {
      "type" : "string",
      "format" : "date-time",
      "description" : "トークンの有効期限(UTC)"
    }
  }
}
        ,
        "400": {
  "description" : "無効なユーザ名、またはパスワード"
}
        
        
      }
    } 
    
  } ,
  
  
  "/pets/{id}/photos": {
    
    "post": {
      "summary": "ペット写真の追加",
      "description":"ペットの写真を追加します。",
      "x-swagger-router-controller": "Pet",
      "tags": ["Pet"],
      "operationId": "uploadFile",
      "parameters": [
        {
  "name" : "id",
  "in" : "path",
  "description" : "写真を追加するペットのID",
  "required" : true,
  "type" : "integer",
  "format" : "int64"
},
        {
  "name" : "file",
  "in" : "formData",
  "description" : "アップロードするファイル",
  "required" : true,
  "type" : "file"
},
        {
  "name" : "additionalMetadata",
  "in" : "formData",
  "description" : "写真の付加情報",
  "required" : false,
  "type" : "string"
}
        
      ],
      "responses": {
        "200": {
  "description" : "アップロード成功",
  "schema" : {
    "$ref" : "#/definitions/ApiResponse"
  }
}
        ,
        "default": {
  "description" : "想定外のエラー",
  "schema" : {
    "$ref" : "#/definitions/ErrorModel"
  }
}
        
        
      }
    } 
    
  } ,
  
  "/pets/{id}": {
    
    "get": {
      "summary": "ペットの取得",
      "description":"指定されたIDを持つペットエンティティを返します。",
      "x-swagger-router-controller": "Pet",
      "tags": ["Pet"],
      "operationId": "findPetById",
      "parameters": [
        {
  "name" : "id",
  "in" : "path",
  "description" : "取得するペットのID",
  "required" : true,
  "type" : "integer",
  "format" : "int64"
}
        
      ],
      "responses": {
        "200": {
  "description" : "ペットエンティティ",
  "schema" : {
    "$ref" : "#/definitions/Pet"
  }
}
        ,
        "default": {
  "description" : "想定外のエラー",
  "schema" : {
    "$ref" : "#/definitions/ErrorModel"
  }
}
        
        
      }
    } ,
    
    "post": {
      "summary": "ペットの更新",
      "description":"フォームからペット情報を更新します。",
      "x-swagger-router-controller": "Pet",
      "tags": ["Pet"],
      "operationId": "updatePetWithFoorm",
      "parameters": [
        {
  "name" : "id",
  "in" : "path",
  "description" : "更新するペットのID",
  "required" : true,
  "type" : "integer",
  "format" : "int64"
},
        {
  "name" : "name",
  "in" : "formData",
  "description" : "更新後のペットの名前",
  "required" : false,
  "type" : "string"
},
        {
  "name" : "status",
  "in" : "formData",
  "description" : "更新後のペットの状態",
  "required" : false,
  "type" : "string"
}
        
      ],
      "responses": {
        "200": {
  "description" : "ペットエンティティ",
  "schema" : {
    "$ref" : "#/definitions/Pet"
  }
}
        ,
        "default": {
  "description" : "想定外のエラー",
  "schema" : {
    "$ref" : "#/definitions/ErrorModel"
  }
}
        
        
      }
    } ,
    
    "delete": {
      "summary": "ペットの削除",
      "description":"指定されたIDを持つペットエンティティを削除します。",
      "x-swagger-router-controller": "Pet",
      "tags": ["Pet"],
      "operationId": "deletePet",
      "parameters": [
        {
  "name" : "id",
  "in" : "path",
  "description" : "ID of pet to delete",
  "required" : true,
  "type" : "integer",
  "format" : "int64"
}
        
      ],
      "responses": {
        "204": {
  "description" : "pet deleted"
}
        ,
        "default": {
  "description" : "想定外のエラー",
  "schema" : {
    "$ref" : "#/definitions/ErrorModel"
  }
}
        
        
      }
    } 
    
  } ,
  
  "/pets": {
    
    "get": {
      "summary": "ペットの検索",
      "description":"ユーザがアクセス可能な全てのペットを返します。\n\n##### 注意\nタグによる絞り込みしかサポートしていません。",
      "x-swagger-router-controller": "Pet",
      "tags": ["Pet"],
      "operationId": "findPets",
      "parameters": [
        {
  "name" : "tags",
  "in" : "query",
  "description" : "絞り込むタグ",
  "required" : false,
  "type" : "array",
  "items" : {
    "type" : "string"
  },
  "collectionFormat" : "multi"
},
        {
  "name" : "limit",
  "in" : "query",
  "description" : "結果の上限",
  "required" : false,
  "type" : "integer",
  "format" : "int32"
}
        
      ],
      "responses": {
        "200": {
  "description" : "ペットエンティティの配列",
  "schema" : {
    "type" : "array",
    "items" : {
      "$ref" : "#/definitions/Pet"
    }
  }
}
        ,
        "default": {
  "description" : "想定外のエラー",
  "schema" : {
    "$ref" : "#/definitions/ErrorModel"
  }
}
        
        
      }
    } ,
    
    "put": {
      "summary": "ペットの更新",
      "description":"既存のペット情報を更新します。",
      "x-swagger-router-controller": "Pet",
      "tags": ["Pet"],
      "operationId": "updatePet",
      "parameters": [
        {
  "in" : "body",
  "name" : "body",
  "description" : "更新するペットオブジェクト",
  "required" : true,
  "schema" : {
    "$ref" : "#/definitions/Pet"
  }
}
        
      ],
      "responses": {
        "400": {
  "description" : "無効なIDが与えられた"
}
        ,
        "404": {
  "description" : "ペットが見つからない"
}
        ,
        "405": {
  "description" : "バリデーション例外"
}
        ,
        "default": {
  "description" : "想定外のエラー",
  "schema" : {
    "$ref" : "#/definitions/ErrorModel"
  }
}
        
        
      }
    } ,
    
    "post": {
      "summary": "ペットの追加",
      "description":"店舗にペットを追加します。重複可。",
      "x-swagger-router-controller": "Pet",
      "tags": ["Pet"],
      "operationId": "addPet",
      "parameters": [
        {
  "in" : "body",
  "name" : "body",
  "description" : "追加するペットオブジェクト",
  "required" : true,
  "schema" : {
    "$ref" : "#/definitions/NewPet"
  }
}
        
      ],
      "responses": {
        "200": {
  "description" : "追加されたペットエンティティ",
  "schema" : {
    "$ref" : "#/definitions/Pet"
  }
}
        ,
        "405": {
  "description" : "無効な入力"
}
        ,
        "default": {
  "description" : "想定外のエラー",
  "schema" : {
    "$ref" : "#/definitions/ErrorModel"
  }
}
        
        
      }
    } 
    
  } ,
  
  
  "/store/orders/{orderId}": {
    
    "get": {
      "summary": "注文の確認",
      "description":"ユーザがアクセス可能な全てのペットを返します。\n\n##### Swagger 側デモAPI (http://petstore.swagger.io/) 利用上の注意\n\n正常なレスポンスを取得するには、 **5以上10未満のIDを指定** してください。\n他の値では例外が発生します。",
      "x-swagger-router-controller": "Store",
      "tags": ["Store"],
      "operationId": "getOrderById",
      "parameters": [
        {
  "name" : "orderId",
  "in" : "path",
  "description" : "取得する注文ID",
  "required" : true,
  "type" : "integer",
  "format" : "int64"
}
        
      ],
      "responses": {
        "200": {
  "description" : "発注成功",
  "schema" : {
    "$ref" : "#/definitions/Order"
  }
}
        ,
        "400": {
  "description" : "無効な注文"
}
        
        
      }
    } 
    
  } ,
  
