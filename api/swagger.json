{
  "swagger": "2.0",
  "info": {
    "title": "Swagger ペットショップ (本番)",
    "description": "これはペットショップサーバの例です。\n\n[Swaggerについてもっと知る](http://swagger.io)、あるいは irc.freenode.net の IRC チャンネル `#swagger` に参加しましょう。\n\nこの例では、認可フィルタの確認用に `special-key` という API キーを利用できます。\n\n本番環境用の追加の説明文です。",
    "version": "1.0.0"
  },
  "produces": ["application/json"],
  "host": "yewton-swagger-playground.herokuapp.com",
  "basePath": "/api",
  "paths": {
  
  "/users": {
    
    "get": {
      "summary": "ペットの検索",
      "description":"hoge",
      "x-swagger-router-controller": "User",
      "tags": ["User"],
      "operationId": "findUser",
      "parameters": [
        {
  "name" : "tags",
  "in" : "query",
  "description" : "絞り込むタグ",
  "required" : false,
  "type" : "array",
  "items" : {
    "type" : "string"
  },
  "collectionFormat" : "multi"
},
        {
  "name" : "limit",
  "in" : "query",
  "description" : "結果の上限",
  "required" : false,
  "type" : "integer",
  "format" : "int32"
}
        
      ],
      "responses": {
        "200": {
  "description" : "ペットエンティティの配列",
  "schema" : {
    "type" : "array",
    "items" : {
      "$ref" : "#/definitions/Pet"
    }
  }
}
        
        
      }
    } 
    
  } ,
  
  
  "/pets/{id}": {
    
    "get": {
      "summary": "ペットの取得",
      "description":"指定されたIDを持つペットエンティティを返します。",
      "x-swagger-router-controller": "Pet",
      "tags": ["Pet"],
      "operationId": "findPetById",
      "parameters": [
        {
  "name" : "id",
  "in" : "path",
  "description" : "取得するペットのID",
  "required" : true,
  "type" : "integer",
  "format" : "int64"
}
        
      ],
      "responses": {
        "200": {
  "description" : "ペットエンティティ",
  "schema" : {
    "$ref" : "#/definitions/Pet"
  }
}
        ,
        "default": {
  "description" : "想定外のエラー",
  "schema" : {
    "$ref" : "#/definitions/ErrorModel"
  }
}
        
        
      }
    } ,
    
    "delete": {
      "summary": "ペットの削除",
      "description":"指定されたIDを持つペットエンティティを削除します。",
      "x-swagger-router-controller": "Pet",
      "tags": ["Pet"],
      "operationId": "deletePet",
      "parameters": [
        {
  "name" : "id",
  "in" : "path",
  "description" : "ID of pet to delete",
  "required" : true,
  "type" : "integer",
  "format" : "int64"
}
        
      ],
      "responses": {
        "204": {
  "description" : "pet deleted"
}
        ,
        "default": {
  "description" : "想定外のエラー",
  "schema" : {
    "$ref" : "#/definitions/ErrorModel"
  }
}
        
        
      }
    } 
    
  } ,
  
  "/pets": {
    
    "get": {
      "summary": "ペットの検索",
      "description":"ユーザがアクセス可能な全てのペットを返します。\n\n##### 注意\nタグによる絞り込みしかサポートしていません。",
      "x-swagger-router-controller": "Pet",
      "tags": ["Pet"],
      "operationId": "findPets",
      "parameters": [
        {
  "name" : "tags",
  "in" : "query",
  "description" : "絞り込むタグ",
  "required" : false,
  "type" : "array",
  "items" : {
    "type" : "string"
  },
  "collectionFormat" : "multi"
},
        {
  "name" : "limit",
  "in" : "query",
  "description" : "結果の上限",
  "required" : false,
  "type" : "integer",
  "format" : "int32"
}
        
      ],
      "responses": {
        "200": {
  "description" : "ペットエンティティの配列",
  "schema" : {
    "type" : "array",
    "items" : {
      "$ref" : "#/definitions/Pet"
    }
  }
}
        ,
        "default": {
  "description" : "想定外のエラー",
  "schema" : {
    "$ref" : "#/definitions/ErrorModel"
  }
}
        
        
      }
    } ,
    
    "post": {
      "summary": "ペットの追加",
      "description":"店舗にペットを追加します。重複可。",
      "x-swagger-router-controller": "Pet",
      "tags": ["Pet"],
      "operationId": "addPet",
      "parameters": [
        {
  "in" : "body",
  "name" : "pet",
  "description" : "追加するペットオブジェクト",
  "required" : true,
  "schema" : {
    "$ref" : "#/definitions/NewPet"
  }
}
        
      ],
      "responses": {
        "200": {
  "description" : "追加されたペットエンティティ",
  "schema" : {
    "$ref" : "#/definitions/Pet"
  }
}
        ,
        "default": {
  "description" : "想定外のエラー",
  "schema" : {
    "$ref" : "#/definitions/ErrorModel"
  }
}
        
        
      }
    } 
    
  } ,
  
  
  "/store": {
    
    "get": {
      "summary": "ペットの検索",
      "description":"hoge",
      "x-swagger-router-controller": "Store",
      "tags": ["Store"],
      "operationId": "findstore",
      "parameters": [
        {
  "name" : "tags",
  "in" : "query",
  "description" : "絞り込むタグ",
  "required" : false,
  "type" : "array",
  "items" : {
    "type" : "string"
  },
  "collectionFormat" : "multi"
},
        {
  "name" : "limit",
  "in" : "query",
  "description" : "結果の上限",
  "required" : false,
  "type" : "integer",
  "format" : "int32"
}
        
      ],
      "responses": {
        "200": {
  "description" : "ペットエンティティの配列",
  "schema" : {
    "type" : "array",
    "items" : {
      "$ref" : "#/definitions/Pet"
    }
  }
}
        
        
      }
    } 
    
  } 
  
  }, "definitions": {
  "Pet": {
  "allOf" : [ {
    "$ref" : "#/definitions/NewPet"
  }, {
    "required" : [ "id" ],
    "properties" : {
      "id" : {
        "type" : "integer",
        "format" : "int64",
        "description" : "識別子"
      }
    }
  } ]
},"NewPet": {
  "required" : [ "name" ],
  "properties" : {
    "name" : {
      "type" : "string",
      "description" : "名前"
    },
    "colors" : {
      "type" : "array",
      "description" : "色",
      "items" : {
        "type" : "string"
      }
    }
  },
  "description" : "新規ペット"
},"ErrorModel": {
  "required" : [ "code", "message" ],
  "properties" : {
    "code" : {
      "type" : "integer",
      "format" : "int32"
    },
    "message" : {
      "type" : "string"
    }
  }
}
  }
}
